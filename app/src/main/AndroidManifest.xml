<?xml version="1.0" encoding="utf-8"?>
<manifest xmlns:android="http://schemas.android.com/apk/res/android"
    package="ia.umkc.career"
    android:versionCode="3"
    android:versionName="1.2" >

    <uses-sdk android:minSdkVersion="8" />

    <uses-permission android:name="android.permission.INTERNET" />
    <uses-permission android:name="android.permission.ACCESS_NETWORK_STATE" />
    <uses-permission android:name="android.permission.ACCESS_COARSE_LOCATION" />
    <uses-permission android:name="android.permission.ACCESS_FINE_LOCATION" />
    <uses-permission android:name="android.permission.CALL_PHONE" />

    <application
        android:icon="@drawable/logo_cs"
        android:theme="@style/CustomTitleTheme">
        <uses-library android:name="com.google.android.maps" />

        <activity
            android:name=".CareerServicesActivity"
            android:label="@string/app_name">
            <intent-filter>
                <action android:name="android.intent.action.MAIN" />
                <category android:name="android.intent.category.LAUNCHER" />
            </intent-filter>
        </activity>
        <activity
            android:name=".MainPageViewer"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".ResumeDevActivity"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".ResumeDevListContents"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".WebViewer"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".VirtualOffice"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".VirtualOfficeContent"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".CustomItemizedOverlay"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".MyOverLay"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".Location"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".RoutePath"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".RooCareerList"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".RooCareerWebViewer"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".CustomLocationManager"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".LocationValue"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        <activity
            android:name=".UpcomingEvents"
            android:windowSoftInputMode="stateHidden"
            android:configChanges="orientation|keyboardHidden|keyboard" >
        </activity>
        
        <activity
            android:name=".EventDetailsActivity" 
            android:configChanges="orientation|keyboardHidden|keyboard" 
            ></activity>
        
        <activity
            android:name=".FeedbackActivity" 
            android:configChanges="orientation|keyboardHidden|keyboard" 
            ></activity>
    </application>

</manifest>